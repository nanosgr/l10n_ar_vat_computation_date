#: model:ir.model.fields,field_description:l10n_ar_vat_computation_date.field_res_company__l10n_ar_vat_credit_account_id
#: model:ir.model.fields,field_description:l10n_ar_vat_computation_date.field_res_config_settings__l10n_ar_vat_credit_account_id
#: model_terms:ir.ui.view,arch_db:l10n_ar_vat_computation_date.res_config_settings_view_form
msgid "VAT Credit Account"
msgstr "Cuenta de IVA Crédito Fiscal"

#. module: l10n_ar_vat_computation_date
#: model:ir.model.fields,help:l10n_ar_vat_computation_date.field_res_company__l10n_ar_vat_credit_account_id
#: model:ir.model.fields,help:l10n_ar_vat_computation_date.field_res_config_settings__l10n_ar_vat_credit_account_id
msgid "Account used for VAT credit in purchase invoices. This account will be replaced when invoices fall in locked periods."
msgstr "Cuenta utilizada para el crédito de IVA en facturas de compra. Esta cuenta será reemplazada cuando las facturas caigan en períodos cerrados."

#. module: l10n_ar_vat_computation_date
#: model:ir.model.fields,field_description:l10n_ar_vat_computation_date.field_res_company__l10n_ar_vat_credit_to_compute_account_id
#: model:ir.model.fields,field_description:l10n_ar_vat_computation_date.field_res_config_settings__l10n_ar_vat_credit_to_compute_account_id
#: model_terms:ir.ui.view,arch_db:l10n_ar_vat_computation_date.res_config_settings_view_form
msgid "VAT Credit To Compute Account"
msgstr "Cuenta de IVA Crédito Fiscal a Computar"

#. module: l10n_ar_vat_computation_date
#: model:ir.model.fields,help:l10n_ar_vat_computation_date.field_res_company__l10n_ar_vat_credit_to_compute_account_id
#: model:ir.model.fields,help:l10n_ar_vat_computation_date.field_res_config_settings__l10n_ar_vat_credit_to_compute_account_id
msgid "Temporary account for VAT credit to be computed in future periods. Used when purchase invoices are posted in locked tax periods."
msgstr "Cuenta temporal para crédito de IVA a computar en períodos futuros. Se utiliza cuando las facturas de compra se contabilizan en períodos fiscales cerrados."

#. module: l10n_ar_vat_computation_date
#: model_terms:ir.ui.view,arch_db:l10n_ar_vat_computation_date.res_config_settings_view_form
msgid "VAT Credit Deferred Computation"
msgstr "Cómputo Diferido de Crédito de IVA"

#. module: l10n_ar_vat_computation_date
#: model_terms:ir.ui.view,arch_db:l10n_ar_vat_computation_date.res_config_settings_view_form
msgid "Configure accounts for VAT credit computation in locked periods (Argentina)"
msgstr "Configurar cuentas para el cómputo de crédito de IVA en períodos cerrados (Argentina)"

#. module: l10n_ar_vat_computation_date
#: model:ir.model.fields,field_description:l10n_ar_vat_computation_date.field_account_move__l10n_ar_vat_computation_date
msgid "VAT Computation Period"
msgstr "Período de Cómputo de IVA"

#. module: l10n_ar_vat_computation_date
#: model:ir.model.fields,help:l10n_ar_vat_computation_date.field_account_move__l10n_ar_vat_computation_date
msgid "Date used to determine in which period the VAT credit will be computed. If the invoice date is within a locked tax period, this will be automatically set to the last day of the month following the lock date."
msgstr "Fecha utilizada para determinar en qué período se computará el crédito de IVA. Si la fecha de la factura está dentro de un período fiscal cerrado, se establecerá automáticamente al último día del mes siguiente a la fecha de bloqueo."

#. module: l10n_ar_vat_computation_date
