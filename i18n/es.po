#. module: l10n_ar_vat_computation_date
#: model:ir.model.fields,help:l10n_ar_vat_computation_date.field_account_move__l10n_ar_vat_source_invoice_id
msgid "Original purchase invoice that generated this VAT adjustment entry"
msgstr "Comprobante de compra original que generó este asiento de ajuste de IVA"

#. module: l10n_ar_vat_computation_date
#: model:ir.model.fields,field_description:l10n_ar_vat_computation_date.field_account_move__l10n_ar_is_vat_adjustment
